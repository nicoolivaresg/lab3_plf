1 REM Esto es un programa que indica si su nÃºmero es positivo o negativo
10 INPUT "INGRESA UN NUMERO: " N
20 PRINT N 
30 IF (N >= 0) THEN PRINT "NUMERO POSITIVO" ELSE PRINT "NUMERO NEGATIVO" ENDIF
70 PRINT "GOOD BYE"
85 GOTO 2
87 WEND
88 ASD$ = CHR(32)
88 ASD$ = RIGHT("perro",3)
88 ASD$ = LEFT("perro",2)
80 END